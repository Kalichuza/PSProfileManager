      <command:name>Backup-PSGlobalProfile</command:name>
      <command:verb>Backup</command:verb>
      <command:noun>PSGlobalProfile</command:noun>
      <command:description>
        <maml:para>Backs up the existing PowerShell profile before making changes.</maml:para>
      </command:description>
    </command:details>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Backup-PSGlobalProfile</maml:name>
        <command:parameter>
          <maml:name>-Verbose</maml:name>
          <maml:description>
            <maml:para>Provides detailed output of the process.</maml:para>
          </command:description>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
    <command:description>
      <maml:para>This command backs up the existing PowerShell profile to a timestamped file.</maml:para>
    </command:description>
    <command:examples>
      <command:example>
        <command:title>Example 1: Backup an existing profile</command:title>
        <maml:code>Backup-PSGlobalProfile</maml:code>
        <maml:remarks>This command creates a backup of the existing PowerShell profile with a timestamp.</maml:remarks>
      </command:example>
    </command:examples>
    <command:inputTypes>
      <command:inputType>
        <dev:typeName>None</dev:typeName>
        <dev:description>
          <maml:para>This command does not take any pipeline input.</maml:para>
        </dev:description>
      </command:inputType>
    </command:inputTypes>
    <command:outputTypes>
      <command:outputType>
        <dev:typeName>None</dev:typeName>
        <dev:description>
          <maml:para>This command does not return any output.</maml:para>
        </dev:description>
      </command:outputType>
    </command:outputTypes>
  </command:command>
</helpItems>
